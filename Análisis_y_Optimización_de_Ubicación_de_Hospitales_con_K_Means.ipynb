        "import pandas as pd\n",
        "import random\n",
        "\n",
        "\n",
        "class KMeansCity:\n",
        "    \"\"\"\n",
        "    Implementa K-Means para optimizar ubicación de hospitales en una ciudad.\n",
        "\n",
        "    La ciudad se modela como matriz m×n con casas distribuidas aleatoriamente.\n",
        "    El algoritmo encuentra K ubicaciones óptimas para hospitales.\n",
        "\n",
        "    Parámetros\n",
        "    ----------\n",
        "    m : int - Número de filas de la ciudad (altura)\n",
        "    n : int - Número de columnas de la ciudad (ancho)\n",
        "    num_houses : int - Número de casas/vecindarios\n",
        "    k_hospitals : int - Número de hospitales a ubicar\n",
        "\n",
        "    Ejemplo\n",
        "    -------\n",
        "    >>> city = KMeansCity(m=15, n=20, num_houses=10, k_hospitals=3)\n",
        "    >>> city.kmeans()\n",
        "    >>> metrics = city.calculate_metrics()\n",
        "    \"\"\"\n",
        "\n",
        "    def __init__(self, m, n, num_houses, k_hospitals):\n",
        "        \"\"\"Inicializa el sistema con parámetros de la ciudad.\"\"\"\n",
        "        self.m = m  # Filas (altura)\n",
        "        self.n = n  # Columnas (ancho)\n",
        "        self.num_houses = num_houses\n",
        "        self.k = k_hospitals\n",
        "        self.houses = []\n",
        "        self.hospitals = []\n",
        "        self.clusters = {}\n",
        "        self.iteration_count = 0\n",
        "\n",
        "\n",
        "    def generate_city(self):\n",
        "        \"\"\"\n",
        "        Genera ciudad con casas en posiciones aleatorias únicas.\n",
        "        Los hospitales se colocan en posiciones vacías (no donde hay casas).\n",
        "        \"\"\"\n",
        "        self.houses = []\n",
        "        used_positions = set()\n",
        "\n",
        "        # Generar casas en posiciones únicas\n",
        "        while len(self.houses) < self.num_houses:\n",
        "            x = random.randint(0, self.m - 1)  # Fila: 0 a m-1\n",
        "            y = random.randint(0, self.n - 1)  # Columna: 0 a n-1\n",
        "            position = (x, y)\n",
        "\n",
        "            if position not in used_positions:\n",
        "                self.houses.append(position)\n",
        "                used_positions.add(position)\n",
        "\n",
        "        # Generar hospitales en posiciones vacías (no donde hay casas)\n",
        "        self.hospitals = []\n",
        "        while len(self.hospitals) < self.k:\n",
        "            x = random.randint(0, self.m - 1)\n",
        "            y = random.randint(0, self.n - 1)\n",
        "            position = (x, y)\n",
        "\n",
        "            # Solo agregar si no está ocupada por casa ni por otro hospital\n",
        "            if position not in used_positions:\n",
        "                self.hospitals.append(position)\n",
        "                used_positions.add(position)\n",
        "\n",
        "\n",
        "    def euclidean_distance(self, point1, point2):\n",
        "        \"\"\"\n",
        "        Calcula distancia euclidiana entre dos puntos.\n",
        "        Fórmula: d = √[(x₂-x₁)² + (y₂-y₁)²]\n",
        "        \"\"\"\n",
        "        dx = point1[0] - point2[0]\n",
        "        dy = point1[1] - point2[1]\n",
        "        return (dx**2 + dy**2)**0.5\n",
        "\n",
        "\n",
        "    def assign_clusters(self):\n",
        "        \"\"\"\n",
        "        PASO 1 de K-Means: Asigna cada casa al hospital más cercano.\n",
        "        Complejidad: O(num_houses × k_hospitals)\n",
        "        \"\"\"\n",
        "        self.clusters = {i: [] for i in range(self.k)}\n",
        "\n",
        "        for house in self.houses:\n",
        "            min_distance = float('inf')\n",
        "            closest_hospital = 0\n",
        "\n",
        "            for i, hospital in enumerate(self.hospitals):\n",
        "                distance = self.euclidean_distance(house, hospital)\n",
        "                if distance < min_distance:\n",
        "                    min_distance = distance\n",
        "                    closest_hospital = i\n",
        "\n",
        "            self.clusters[closest_hospital].append(house)\n",
        "\n",
        "\n",
        "    def update_hospitals(self):\n",
        "        \"\"\"\n",
        "        PASO 2 de K-Means: Actualiza hospitales al centroide de cada cluster.\n",
        "        Centroide: (promedio_x, promedio_y) de todas las casas del cluster.\n",
        "        \"\"\"\n",
        "        new_hospitals = []\n",
        "\n",
        "        for i in range(self.k):\n",
        "            if self.clusters[i]:\n",
        "                cluster_points = self.clusters[i]\n",
        "                sum_x = sum(point[0] for point in cluster_points)\n",
        "                sum_y = sum(point[1] for point in cluster_points)\n",
        "                n_points = len(cluster_points)\n",
        "\n",
        "                new_x = max(0, min(self.m - 1, round(sum_x / n_points)))\n",
        "                new_y = max(0, min(self.n - 1, round(sum_y / n_points)))\n",
        "\n",
        "                new_hospitals.append((new_x, new_y))\n",
        "            else:\n",
        "                new_hospitals.append(self.hospitals[i])\n",
        "\n",
        "        return new_hospitals\n",
        "\n",
        "\n",
        "    def kmeans(self, max_iterations=100, verbose=True):\n",
        "        \"\"\"\n",
        "        Ejecuta algoritmo K-Means completo.\n",
        "\n",
        "        Proceso:\n",
        "            1. Genera ciudad inicial\n",
        "            2. Repite hasta convergencia:\n",
        "               - Asigna casas a hospitales cercanos\n",
        "               - Actualiza hospitales a centroides\n",
        "            3. Converge cuando hospitales no cambian de posición\n",
        "\n",
        "        Parámetros\n",
        "        ----------\n",
        "        max_iterations : int - Límite de iteraciones (default: 100)\n",
        "        verbose : bool - Mostrar progreso (default: True)\n",
        "        \"\"\"\n",
        "        self.generate_city()\n",
        "        self.iteration_count = 0\n",
        "\n",
        "        for iteration in range(max_iterations):\n",
        "            old_hospitals = self.hospitals.copy()\n",
        "\n",
        "            self.assign_clusters()\n",
        "            self.hospitals = self.update_hospitals()\n",
        "\n",
        "            self.iteration_count = iteration + 1\n",
        "\n",
        "            if old_hospitals == self.hospitals:\n",
        "                if verbose:\n",
        "                    print(f\"✓ Convergencia alcanzada en iteración {self.iteration_count}\")\n",
        "                break\n",
        "        else:\n",
        "            if verbose:\n",
        "                print(f\"Máximo de {max_iterations} iteraciones alcanzado\")\n",
        "\n",
        "\n",
        "    def calculate_metrics(self):\n",
        "        \"\"\"\n",
        "        Calcula métricas de calidad del clustering.\n",
        "\n",
        "        Retorna\n",
        "        -------\n",
        "        dict con métricas: distancia promedio/min/max, desviación estándar,\n",
        "        mediana, cobertura, número de iteraciones.\n",
        "        \"\"\"\n",
        "        all_distances = []\n",
        "\n",
        "        for i in range(self.k):\n",
        "            for house in self.clusters[i]:\n",
        "                distance = self.euclidean_distance(house, self.hospitals[i])\n",
        "                all_distances.append(distance)\n",
        "\n",
        "        distances_series = pd.Series(all_distances)\n",
        "\n",
        "        return {\n",
        "            'casas_totales': len(self.houses),\n",
        "            'casas_cubiertas': len(all_distances),\n",
        "            'hospitales': self.k,\n",
        "            'distancia_promedio': distances_series.mean() if len(distances_series) > 0 else 0,\n",
        "            'distancia_maxima': distances_series.max() if len(distances_series) > 0 else 0,\n",
        "            'distancia_minima': distances_series.min() if len(distances_series) > 0 else 0,\n",
        "            'desviacion_std': distances_series.std() if len(distances_series) > 0 else 0,\n",
        "            'mediana': distances_series.median() if len(distances_series) > 0 else 0,\n",
        "            'cobertura_porcentaje': (len(all_distances) / len(self.houses)) * 100 if len(self.houses) > 0 else 0,\n",
        "            'iteraciones': self.iteration_count\n",
        "        }\n",
        "\n",
        "\n",
        "    def get_results_dataframe(self):\n",
        "        \"\"\"\n",
        "        Genera DataFrame con resultados detallados del clustering.\n",
        "        Columnas: Casa_X, Casa_Y, Hospital_ID, Hospital_X, Hospital_Y, Distancia\n",
        "        \"\"\"\n",
        "        results_data = []\n",
        "\n",
        "        for i in range(self.k):\n",
        "            for house in self.clusters[i]:\n",
        "                distance = self.euclidean_distance(house, self.hospitals[i])\n",
        "                results_data.append({\n",
        "                    'Casa_X': house[0],\n",
        "                    'Casa_Y': house[1],\n",
        "                    'Hospital_ID': i + 1,\n",
        "                    'Hospital_X': self.hospitals[i][0],\n",
        "                    'Hospital_Y': self.hospitals[i][1],\n",
        "                    'Distancia': round(distance, 3)\n",
        "                })\n",
        "\n",
        "        return pd.DataFrame(results_data)\n",
        "\n",
        "\n",
        "    def verificar_casas(self):\n",
        "        \"\"\"\n",
        "        Verifica que todas las casas estén presentes en el sistema\n",
        "        y muestra cuáles son visibles y cuáles están ocultas por hospitales.\n",
        "        \"\"\"\n",
        "        print(\"\\n\" + \"=\"*70)\n",
        "        print(\" VERIFICACIÓN DE CASAS\")\n",
        "        print(\"=\"*70)\n",
        "\n",
        "        casas_visibles = 0\n",
        "        casas_ocultas = 0\n",
        "\n",
